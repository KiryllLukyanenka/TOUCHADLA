    <fields>
        <fullName>CFCR_Current_Stage_In_Funnel_Date__c</fullName>
        <description>Displays funnel date for current stage in the funnel. Used for report filters.</description>
        <externalId>false</externalId>
        <formula>IF ( 
OR( 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - No Action Required&quot; ),
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Merged&quot; ),  
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Unqualified Forever&quot; ),
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Pending&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Recycled&quot; ),  
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Already Engaged&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Deleted&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Lost&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Won&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Unmapped&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Legacy&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Not a Response&quot;)),NULL ,
IF(VO__c = TRUE, VO_Date__c, 
IF( 
AND( VO__c = FALSE, FCRM__FCR_SQR__c = TRUE), FCRM__FCR_SQR_Date__c, 
IF( 
AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = TRUE ),  FCRM__FCR_SAR_Date__c , 
IF( 
AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = FALSE,FCRM__FCR_QR__c = TRUE ),  FCRM__FCR_QR_Date__c , 
IF( 
AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = FALSE,FCRM__FCR_QR__c = FALSE, FCRM__FCR_Inquiry_Target__c = TRUE ),  FCRM__FCR_Inquiry_Target_Date__c , 
 NULL ) 
)))))</formula>
        <inlineHelpText>Displays funnel date for current stage in the funnel. Used for report filters.</inlineHelpText>
        <label>Current Stage In Funnel Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>CFCR_Current_Stage_in_Funnel__c</fullName>
        <description>indicates if a response is active in the funnel or not.</description>
        <externalId>false</externalId>
        <formula>IF ( 
OR( 
ISPICKVAL(Status_Reason__c,&quot;SLA Expired - QL&quot; ), 
ISPICKVAL(Status_Reason__c,&quot;SLA Expired - SAL&quot; ) 
),&quot;SLA Expired&quot;, 
IF( 
OR( 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - No Action Required&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Merged&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Pending&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Unmapped&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Legacy&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Not a Response&quot;)),&quot;Not QL Eligible&quot; , 
IF( 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Already Engaged&quot; ),&quot;Already Engaged&quot;, 

IF( 
OR( 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Deleted&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Lost&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Resolved - Opportunity Won&quot; )), &quot;6. VO CLOSED&quot;, 

IF( 
OR( 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Unqualified Forever&quot; ), 
ISPICKVAL(FCRM__FCR_Response_Status__c, &quot;Recycled&quot; )),&quot;7. SRL&quot;, 

IF(VO__c = TRUE, &quot;5. VO&quot;, 
IF(AND( VO__c = FALSE, FCRM__FCR_SQR__c = TRUE), &quot;4. SQO&quot;, 
IF(AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = TRUE ), &quot;3. SAL&quot;, 
IF(AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = FALSE,FCRM__FCR_QR__c = TRUE ), &quot;2. QL&quot;, 
IF(AND( VO__c = FALSE, FCRM__FCR_SQR__c = FALSE, FCRM__FCR_SAR__c = FALSE,FCRM__FCR_QR__c = FALSE, FCRM__FCR_Inquiry_Target__c = TRUE ), &quot;1. Inquiry&quot;, 
&quot;UNDEFINED&quot;) 
)))))))))</formula>
        <inlineHelpText>indicates if a response is active in the funnel or not.</inlineHelpText>
        <label>Current Stage in Funnel</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
