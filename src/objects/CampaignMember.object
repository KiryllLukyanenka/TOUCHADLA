    <fields>
        <fullName>FCCRM_Member_Threshold__c</fullName>
        <description>Score threshold evaluated in addition to the related lead/contact and campaign threshold values.</description>
        <externalId>false</externalId>
        <formula>IF( 
	OR ( 
		ISPICKVAL( 
			Campaign.Campaign_Type__c ,&quot;Contact Me&quot;), /* If campaign type is contact me, set to true*/ 
		AND(ISPICKVAL(Status,&quot;Visited Booth - P1 (Hot)&quot;),
			Do_Not_MQL__c=FALSE),
		AND(
			ISPICKVAL( 
			Campaign.Campaign_Type__c,&quot;Self-Serve&quot;),
			Profile_Score_Current__c &lt;&gt; &quot;D&quot;),
		AND ( 
				Do_Not_MQL__c=FALSE, /* Do not QL is False */ 
				NOT(ISPICKVAL(Campaign.Campaign_Type__c,&quot;Email&quot;)),
				NOT(ISPICKVAL(Status,&quot;Visited Booth - P3 (Cold)&quot;)), /* Status is not P3 */ 
				Profile_Score_Current__c &lt;&gt; &quot;D&quot;, /* Do not QL if score is &apos;d&apos; */		 
				NOT( 
					AND(	/*Campaign = &quot;Content Marketing&quot; and Profile Score = &quot;C&quot; */ 
						ISPICKVAL(Campaign.Campaign_Type__c,&quot;Content Marketing&quot;), 
						Profile_Score_Current__c = &quot;C&quot; 
					)), 
				NOT( 
					AND(	/*Campaign = &quot;Webinar&quot; and Profile Score = &quot;C&quot;*/ 
						ISPICKVAL( 
							Campaign.Campaign_Type__c,&quot;Webinar&quot;), 
						ISPICKVAL(Status,&quot;No Show&quot;) 
					)), 
				NOT( 
					AND(	
						ISPICKVAL( 
							Campaign.Campaign_Type__c,&quot;Event&quot;), 
						ISPICKVAL( Campaign.Campaign_Sub_Type__c,&quot;Field Event&quot;), 
						ISPICKVAL(Status,&quot;Attended&quot;) 
					)) 
		) 
	),2000, -2000)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Score threshold evaluated in addition to the related lead/contact and campaign threshold values.</inlineHelpText>
        <label>FCCRM Member Threshold</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
