    <validationRules>
        <fullName>Agreement_date_is_required</fullName>
        <active>false</active>
        <description>If &quot;Subject to Agreement&apos; field is checked, require date to be entered</description>
        <errorConditionFormula>AND 
(We_have_read_agreed_to_terms__c = TRUE,
ISBLANK  (Signed_Agreement_Date__c )
)</errorConditionFormula>
        <errorMessage>Please enter date of signed agreement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMXB__End_Date_Must_Be_Greater_Than_Start_Date</fullName>
        <active>false</active>
        <errorConditionFormula>BMXB__Start_Date__c &gt;  BMXB__End_Date2__c</errorConditionFormula>
        <errorDisplayField>BMXB__End_Date2__c</errorDisplayField>
        <errorMessage>End Date cannot be before Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMXB__No_Negative_Number_of_Terms</fullName>
        <active>false</active>
        <description>You cannot enter a negative number for Number of Terms (in Months)</description>
        <errorConditionFormula>BMXB__Contract_Term_Months__c  &lt; 0</errorConditionFormula>
        <errorMessage>You cannot enter a negative Number of Terms (in Months)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Eula_Required_on_Opportunity</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(TEXT(Opportunity.Project_Region__c))</errorConditionFormula>
        <errorMessage>Add Project Country and Project EULA on Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Net_60_Payment_Terms</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Payment_Terms__c,&quot;Net 60&quot;), 
OR( 
ISPICKVAL(PRIORVALUE(Payment_Terms__c), &quot;Net 14&quot;), 
ISPICKVAL(PRIORVALUE(Payment_Terms__c), &quot;Net 30&quot;), 
ISPICKVAL(PRIORVALUE(Payment_Terms__c), &quot;Net 45&quot;)), 
$Profile.Name &lt;&gt; &quot;AppD Sys Admin&quot;, 
$Profile.Name &lt;&gt; &quot;AppD Order Ops&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;AppD Legal&quot;, 
$Profile.Name &lt;&gt; &quot;AppD Finance&quot;)</errorConditionFormula>
        <errorDisplayField>Payment_Terms__c</errorDisplayField>
        <errorMessage>Please Choose Net 30 or Net 45 or contact Order Ops</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Signed_EULA_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( EULA_or_Agreement_Type__c ,&quot;Signed EULA&quot;) &amp;&amp;  ISBLANK( Signed_Agreement_Date__c )</errorConditionFormula>
        <errorDisplayField>Signed_Agreement_Date__c</errorDisplayField>
        <errorMessage>Signed Agreement Date is required for signed EULA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VAR_Contract_Date_is_Required</fullName>
        <active>false</active>
        <description>If &quot;Is_Partner&quot; is checked,  VAR Contract Date require date to be entered</description>
        <errorConditionFormula>AND 
( Is_Partner__c = TRUE,
ISBLANK  ( VAR_Contract_Date__c )
)</errorConditionFormula>
        <errorMessage>Please enter VAR Contract Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_required_for_One_Time_Discount</fullName>
        <active>false</active>
        <description>A value must be entered for One Time Discount. 0 may be entered.</description>
        <errorConditionFormula>ISNULL( One_Time_Discount_Percentage__c )</errorConditionFormula>
        <errorDisplayField>One_Time_Discount_Percentage__c</errorDisplayField>
        <errorMessage>A value must be entered for One Time Discount. 0 may be entered.</errorMessage>
    </validationRules>
