    <fields>
        <fullName>BMXB__End_Date__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>DATE(YEAR( BMXB__Start_Date__c ) + floor((month(BMXB__Start_Date__c) + BMXB__Contract_Term_Months__c )/12) + 
if(and(month(BMXB__Start_Date__c)=12,BMXB__Contract_Term_Months__c&gt;=12),-1,0), 
if( mod( month(BMXB__Start_Date__c) + BMXB__Contract_Term_Months__c , 12 ) = 0, 12 , mod( month(BMXB__Start_Date__c) +BMXB__Contract_Term_Months__c, 12 )), 
min( day(BMXB__Start_Date__c), 
case( 
max( mod( month(BMXB__Start_Date__c) + BMXB__Contract_Term_Months__c , 12 ) , 1), 
9,30, 
4,30, 
6,30, 
11,30, 
2,28+IF(MOD(YEAR( BMXB__Start_Date__c ) + floor((month(BMXB__Start_Date__c) + BMXB__Contract_Term_Months__c )/12) + 
if(and(month(BMXB__Start_Date__c)=12,BMXB__Contract_Term_Months__c&gt;=12),-1,0), 4) = 0, 1, 0), 
31 
) 
) 
)-1 + ROUND( (BMXB__Contract_Term_Months__c - FLOOR(BMXB__Contract_Term_Months__c)) * 
CASE( 
MOD( MONTH(BMXB__Start_Date__c) + FLOOR(BMXB__Contract_Term_Months__c) , 12 ), 
9,30, 
4,30, 
6,30, 
11,30, 
2,28 + IF(MOD(YEAR( BMXB__Start_Date__c ) + floor((month(BMXB__Start_Date__c) + BMXB__Contract_Term_Months__c )/12) + 
if(and(month(BMXB__Start_Date__c)=12,BMXB__Contract_Term_Months__c&gt;=12),-1,0), 4) = 0, 1, 0), 
31 
), 0 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>End Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
