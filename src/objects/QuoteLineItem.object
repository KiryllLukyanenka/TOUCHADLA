    <validationRules>
        <fullName>Months_Required_For_Monthly_Products</fullName>
        <active>false</active>
        <description>If the product is monthly then the Months field must be greater than 0.</description>
        <errorConditionFormula>Months__c  = 0 &amp;&amp;  Product2.Monthly__c = TRUE &amp;&amp;  Quote.Quote_Created__c = TRUE  || ISNULL(Months__c) &amp;&amp;  Product2.Monthly__c = TRUE &amp;&amp;  Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>Months__c</errorDisplayField>
        <errorMessage>If the product is monthly then the Months field must be greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Discount_on_Microservices</fullName>
        <active>false</active>
        <description>Microservices are not to be discounted</description>
        <errorConditionFormula>AND 
 (Product2.No_Discount__c  = TRUE,Discount &gt; 0)</errorConditionFormula>
        <errorMessage>Microservices cannot be discounted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Month_Update_for_RapidStart_Product</fullName>
        <active>false</active>
        <description>Stops User Profiles except for Legal Customer Success, and OrderOps from updating the Month value to anything other than 3 for RapidStart Adoption Offering products.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;AppD Customer Success&quot;,
Product2.ProductCode = &quot;ENB-RSA-SERV-FY16&quot;,
NOT(ISNEW()), Months__c &lt;&gt; 3)</errorConditionFormula>
        <errorDisplayField>Months__c</errorDisplayField>
        <errorMessage>Months value must equal 3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_One_Time_Disc_if_product_is_not_Elig</fullName>
        <active>false</active>
        <description>No_Volume_Discount_if_product_is_not_Eligible</description>
        <errorConditionFormula>ISPICKVAL( Product2.Partner_Margin_Applied__c , &quot;False&quot;) &amp;&amp;  One_Time_Discount__c   &gt; 0 &amp;&amp; Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>One_Time_Discount__c</errorDisplayField>
        <errorMessage>This product is not eligible for a One Time Discount. Please enter 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Volume_Discount_if_product_is_not_Eli</fullName>
        <active>false</active>
        <description>No_Volume_Discount_if_product_is_not_Eligible</description>
        <errorConditionFormula>ISPICKVAL(Product2.Family, &quot;Perpetual Support&quot;) &amp;&amp; Volume_Discount__c  &gt; 0 &amp;&amp; Quote.Quote_Created__c = TRUE || ISPICKVAL(Product2.Family, &quot;Training&quot;) &amp;&amp; Volume_Discount__c  &gt; 0 &amp;&amp; Quote.Quote_Created__c = TRUE || ISPICKVAL(Product2.Family, &quot;Premium Subscriptions Support&quot;) &amp;&amp; Volume_Discount__c  &gt; 0</errorConditionFormula>
        <errorDisplayField>Volume_Discount__c</errorDisplayField>
        <errorMessage>This product is not eligible for a Volume Discount. Volume Discount must be set to 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Monthly_Product</fullName>
        <active>false</active>
        <description>Months can not be entered if the Product is not Monthly.</description>
        <errorConditionFormula>Months__c  &gt; 0 &amp;&amp; Product2.Monthly__c = FALSE &amp;&amp; Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>Months__c</errorDisplayField>
        <errorMessage>Months cannot be entered if the Product is not Monthly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Time_Discount_NOT_NULL</fullName>
        <active>false</active>
        <description>A value must be entered for One Time Discount. 0 may be entered.</description>
        <errorConditionFormula>ISNULL( One_Time_Discount__c ) &amp;&amp; Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>One_Time_Discount__c</errorDisplayField>
        <errorMessage>A value must be entered for One Time Discount %. 0 may be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Perpetual_Support_Renewal_Price_NOT_NULL</fullName>
        <active>false</active>
        <description>A value must be entered for Monthly Perpetual Support Renewal. 0 may be entered.</description>
        <errorConditionFormula>ISNULL(Perpetual_Support_Renewal_Price__c) &amp;&amp; Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>Perpetual_Support_Renewal_Price__c</errorDisplayField>
        <errorMessage>A value must be entered for Monthly Perpetual Support Renewal. 0 may be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Perpetual_Support_Renewal_Price_Not_Appl</fullName>
        <active>false</active>
        <description>A value should only be entered in the Monthly Perpetual Support Renewal field if the Perpetual Support Type field on the Product equals Renewal.</description>
        <errorConditionFormula>Perpetual_Support_Renewal_Price__c  &gt; 0 &amp;&amp;  NOT (ISPICKVAL( Product2.Perpetual_Support_Type__c, &quot;Renewal&quot;)) &amp;&amp; Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>Perpetual_Support_Renewal_Price__c</errorDisplayField>
        <errorMessage>Monthly Perpetual Support Renewal Price must be 0 unless the product is Support Renewal for Perpetual Licenses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Perpetual_Support_Renewal_Price_Required</fullName>
        <active>false</active>
        <description>A value must be entered in the Monthly Perpetual Support Renewal field if the Perpetual Support Type field on the Product equals Renewal.</description>
        <errorConditionFormula>ISNULL( Perpetual_Support_Renewal_Price__c ) &amp;&amp;  (ISPICKVAL( Product2.Perpetual_Support_Type__c, &quot;Renewal&quot;)) &amp;&amp;  Quote.Quote_Created__c = TRUE || Perpetual_Support_Renewal_Price__c = 0 &amp;&amp;  (ISPICKVAL( Product2.Perpetual_Support_Type__c, &quot;Renewal&quot;)) &amp;&amp;  Quote.Quote_Created__c = TRUE</errorConditionFormula>
        <errorDisplayField>Perpetual_Support_Renewal_Price__c</errorDisplayField>
        <errorMessage>Monthly Perpetual Support Renewal Price must be greater than 0 if the product is Support Renewal for Perpetual Licenses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Price_on_CoTerm</fullName>
        <active>false</active>
        <errorConditionFormula>AND(Product2.Co_Term__c=FALSE,Product2.Discount_SKU__c=FALSE,  ISCHANGED(UnitPrice))</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>Please delete the Line Item from the Quote, and add again at the correct quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_price_entry_for_Renewal_products</fullName>
        <active>false</active>
        <description>Sales Reps can only enter price for Renewal products</description>
        <errorConditionFormula>AND(
NOT(
ISPICKVAL( Product2.Family ,&quot;Renewal&quot;)
),
ISCHANGED (UnitPrice)
)</errorConditionFormula>
        <errorMessage>Changes to the Sales price are not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volume_Discount_Cannot_Equal_NULL</fullName>
        <active>false</active>
        <description>A value must be entered for Volume Discount. 0 can be entered. 
This is needed for the Discount Amount fields on the Quote to calculate.</description>
        <errorConditionFormula>ISNULL( Volume_Discount__c )</errorConditionFormula>
        <errorDisplayField>Volume_Discount__c</errorDisplayField>
        <errorMessage>A value must be entered for Volume Discount. 0 can be entered.</errorMessage>
    </validationRules>
