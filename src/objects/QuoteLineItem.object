        <linkType>javascript</linkType>
        <masterLabel>Manage Line Items</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>if({!$Setup.BMXB__Pricing_Settings__c.BMXB__Optional_Line_Items_Enabled__c}){ 
var url =&apos;{!$Site.Prefix}&apos;+&apos;/apex/&apos;; 
if(&apos;{!$Setup.BMXB__Namespace__c.BMXB__pricing__c}&apos;.length &gt; 0){ 
url +=&apos;{!$Setup.BMXB__Namespace__c.BMXB__pricing__c}&apos;+&apos;__&apos;; 
} 
url += &apos;OptionalItemManagement?primaryQuote=&apos;; 

if(&apos;{!Quote.BMXP__Primary_QuoteId__c}&apos;.length &gt;0){ 
url += &apos;{!Quote.BMXP__Primary_QuoteId__c}&apos;; 

} 
else{url += &apos;{!Quote.Id}&apos;;} 

window.location.href = url; 
} 

else{ 
alert(&apos;Optional Line Item Management is not enabled. Contact administrator or check Custom Settings.&apos;); 
}</url>
    </webLinks>
